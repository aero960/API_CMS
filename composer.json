{
  "name": "composer/test",
  "description": "composer test app",
  "keywords": [
    "test"
  ],
  "type": "project",
  "version": "0.0.3",
  "license": "",
  "autoload": {
    "psr-4": {
      "APP\\": "src/"
    },
    "classmap": [
