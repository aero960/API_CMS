      "src/interface/interface.php",


      "src/class/routes.php",
      "src/controller/controller_route.php",
      "src/class/element.php",
      "src/class/security.php",
      "src/class/plugins.php",
      "src/class/view.php",
      "src/class/information.php",

      "src/class/operations/crud.php",
      "src/class/operations/schema.php",
      "src/class/operations/product.php",
      "src/class/authentication.php",
      "src/class/converter.php",


